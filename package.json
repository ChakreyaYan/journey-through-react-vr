    "eslint-plugin-jest": "^21.15.0",
    "eslint-plugin-react": "^7.7.0",
    "husky": "^0.14.3",
    "jest": "^19.0.2",
    "metro-bundler": "^0.22.1",
    "prettier": "^1.11.1",
